	"version": "0.2.0",
	"configurations": [
		{
			"name": "gda sample",
			"program": "${workspaceFolder}/src/myGoogleDriveApi.js",
			"request": "launch",
			"skipFiles": ["<node_internals>/**"],
			"type": "pwa-node",
			"console": "integratedTerminal"
		},
		{
			"type": "pwa-chrome",
			"request": "launch",
			"name": "localhost",
			"url": "http://localhost:3000",
			"webRoot": "${workspaceFolder}"
		}
	]
}
